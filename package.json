{
  "name": "react-note-app",
  "version": "0.15.1",
  "private": true,
  "description": "Simple React serverless application to manage your notes.",
  "keywords": [
    "react",
    "serverless",
    "firebase"
  ],
  "bugs": {
    "url": "https://github.com/pawelrusak/react-note-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pawelrusak/react-note-app.git"
  },
  "license": "MIT",
  "author": {
    "name": "Adam Romański",
    "email": "helloroman.vlog@gmail.com",
    "url": "https://helloroman.pl/"
  },
  "contributors": [
    {
      "name": "Paweł Rusak",
      "url": "https://github.com/pawelrusak"
    }
  ],
  "scripts": {
    "build": "craco build",
    "eject": "react-scripts eject",
    "start": "craco start",
    "test": "craco test",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "prepare": "husky install",
    "pre-commit": "tsc && lint-staged",
    "pre-push": "npm test -- --watchAll=false",
    "commit-msg": "npx commitlint --edit \"$1\""
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --config .prettierrc --write",
      "eslint --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.3.1",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/storybook-react-router": "^1.0.1",
    "@types/styled-components": "^5.1.9",
    "firebase": "^8.6.2",
    "formik": "^2.2.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-use": "^17.2.4",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.3.0",
    "typescript": "^4.2.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@craco/craco": "^6.1.2",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/node-logger": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.9",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/redux-mock-store": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "babel-eslint": "^10.1.0",
    "craco-alias": "^3.0.1",
    "cypress": "^7.3.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jest-dom": "^3.9.0",
    "eslint-plugin-jest-formatting": "^3.0.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.5.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "react-redux": "^7.2.4",
    "redux-mock-store": "^1.5.4",
    "storybook-react-router": "^1.0.8"
  }
}
