{
  "extends": [
    "airbnb", 
    "airbnb/hooks",
    "prettier",
    "prettier/react"
  ],
  "env": {
    "jest": true
  },
  "globals": {
    "document": true
  },
  "rules": {
    "react/jsx-filename-extension": [
      1, 
      { 
        "extensions": [
          ".js"
        ] 
      }
    ],
    "react/react-in-jsx-scope": 0
  }
}